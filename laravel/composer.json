{
    "name": "lumi-laravel-boilerplate/back",
    "type": "project",
    "description": "Lumi Laravel Boilerplate",
    "keywords": [],
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "intervention/image": "^2.4",
        "laravel/framework": "10.*",
        "php-http/guzzle7-adapter": "^1.0",
        "radiantabyss/lumi-laravel-auth": "3.*",
        "radiantabyss/lumi-laravel-core": "1.*",
        "symfony/http-client": "^6.4",
        "symfony/mailgun-mailer": "^6.4",
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^10.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/Helper.php"
        ],
        "classmap": [
            "database/seeds"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('env.php') || copy('env.example.php', 'env.php');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
